{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": false,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es5", "es6", "dom", "es2019"],
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "exclude": [
    "./node_modules/*"
  ]
}
